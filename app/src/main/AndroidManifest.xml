<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkinggarage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.RatesActivity"
            android:label="@string/title_activity_rates"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".view.ReceiptActivity"
            android:label="@string/title_activity_receipt"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.TicketActivity"
            android:label="@string/title_activity_ticket"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.ParkActivity"
            android:label="@string/title_activity_park"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.AttendantActivity"
            android:label="@string/title_activity_attendant"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.AttendantsListActivity"
            android:label="@string/title_activity_attendants_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.GarageSetupActivity"
            android:label="@string/title_activity_garage_setup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.AccountSetupActivity"
            android:label="@string/title_activity_manager_account_setup"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.ManagerActivity"
            android:label="@string/title_activity_manager"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>